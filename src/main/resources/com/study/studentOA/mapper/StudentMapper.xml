<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.studentOA.mapper.StudentMapper">

    <select id="getStudentsByClassId" resultType="com.study.studentOA.entity.Student">
        select * from tb_student where tb_student.student_class = #{classId};
    </select>

</mapper>